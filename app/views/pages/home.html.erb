<h1 class="title">~ All Dishes ~</h1>

<div class="container">
  <div class="row">
    <% @dishes.each do |dish| %>
        <div class="col-sm-3">
          <%= link_to('Dish', dishes_path(name: dish.name), image_tag dish.image, class: "img-thumbnail img-fluid", style: "height:200px;margin-bottom:20px") %>
      </div>
    <% end %>
  </div>
</div>
